@model IEnumerable<HavuzProjesi.Models.YonetimKurulu>
@{
    ViewBag.Title = "Yönetim Kurulu";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Aciklama)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.Raw(item.Aciklama)
            </td>
            <td>
                @Html.ActionLink("Düzenle", "Edit", new { id = item.YonetimKuruluId })

            </td>
        </tr>
    }

</table>